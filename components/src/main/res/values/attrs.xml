<?xml version="1.0" encoding="utf-8"?>
<resources>

    <attr name="hintType">
        <flag name="none" value="0" />
        <flag name="fixed" value="1" />
        <flag name="fixedToLeft" value="2" />
    </attr>

    <declare-styleable name="InputLayoutView">
        <attr name="android:text" />
        <attr name="android:hint" />
        <attr name="android:gravity" />
        <attr name="android:imeOptions" />
        <attr name="android:nextFocusForward" />
        <attr name="android:nextFocusUp" />
        <attr name="android:nextFocusDown" />
        <attr name="android:nextFocusRight" />
        <attr name="android:nextFocusLeft" />
        <attr name="android:maxLength" />
        <attr name="android:inputType"/>
        <attr name="android:enabled" />
        <attr name="android:focusable" />
        <attr name="android:maxLines"/>
        <attr name="passwordToggleEnabled" />
        <attr name="hintAnimationEnabled" />
        <attr name="hintType" />
        <attr name="hintWidth" format="dimension"/>
        <attr name="counterEnabled" />
        <attr name="counterMaxLength" />
        <attr name="mask" format="string" />
        <attr name="notMaskedSymbol" format="string" />
        <attr name="editable" format="boolean" />
    </declare-styleable>

</resources>
